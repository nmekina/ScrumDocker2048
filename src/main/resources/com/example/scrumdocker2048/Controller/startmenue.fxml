<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="327.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.scrumdocker2048.Controller.StartmenueController">
   <children>
      <Label layoutX="130.0" layoutY="73.0" text="2048">
         <font>
            <Font size="31.0" />
         </font>
      </Label>
      <MenuBar layoutX="14.0" layoutY="14.0">
         <menus>
            <Menu mnemonicParsing="false" text="Setings">
               <items>
                  <MenuItem fx:id="settingsbtn" mnemonicParsing="false" onAction="#settingsbtnac" text="Settings" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem fx:id="helpbtn" mnemonicParsing="false" onAction="#helpbtnac" text="Game Help" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Highscore">
               <items>
                  <MenuItem fx:id="highscoreac" mnemonicParsing="false" onAction="#highscorebtnac" text="Highscore" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <Button fx:id="startgamebtn" layoutX="116.0" layoutY="200.0" mnemonicParsing="false" onAction="#startgamebtnac" text="Start the game" />
   </children>
</AnchorPane>
