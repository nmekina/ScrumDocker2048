<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="327.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.scrumdocker2048.Controller.StartmenueController">
    <children>
        <Label fx:id="labelName" layoutX="123.0" layoutY="62.0" prefHeight="71.0" prefWidth="109.0" styleClass="labelName" text="2048">
            <font>
                <Font size="31.0" />
            </font>
        </Label>
        <MenuBar>
            <menus>
                <Menu mnemonicParsing="false" text="Highscore">
                    <items>
                        <MenuItem fx:id="MenueBarHighscore" mnemonicParsing="false" onAction="#MenueBarHighscoreClicked" text="Highscore" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <Button fx:id="startgamebtn" layoutX="94.0" layoutY="177.0" mnemonicParsing="false" onAction="#ButtonStartGameClicked" styleClass="startgamebtn" text="Start the game" />
    </children>
</AnchorPane>
