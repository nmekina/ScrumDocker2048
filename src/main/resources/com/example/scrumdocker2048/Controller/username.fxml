<AnchorPane prefHeight="249.0" prefWidth="334.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.scrumdocker2048.Controller.UsernameController">
   <children>
      <ChoiceBox fx:id="dropdownUsername" layoutX="130.0" layoutY="79.0" prefWidth="150.0" />
      <TextField fx:id="txtUsername" layoutX="130.0" layoutY="116.0" />
      <PasswordField fx:id="txtPassword" layoutX="131.0" layoutY="153.0" prefHeight="25.0" prefWidth="149.0" />
      <Label layoutX="129.0" layoutY="12.0" styleClass="labelName" stylesheets="@style.css" text="2048">
         <font>
            <Font name="System Bold" size="33.0" />
         </font>
      </Label>
      <Label layoutX="44.0" layoutY="116.0" prefHeight="25.0" prefWidth="62.0" text="username:" />
      <Label layoutX="44.0" layoutY="153.0" prefHeight="25.0" prefWidth="62.0" text="password:" />
      <Label layoutX="44.0" layoutY="79.0" prefHeight="25.0" prefWidth="70.0" text="select user:" />
      <Button fx:id="btnPlay" layoutX="249.0" layoutY="188.0" mnemonicParsing="false" onAction="#btnPlayPressed" styleClass="btnPlay" text="Play" />
